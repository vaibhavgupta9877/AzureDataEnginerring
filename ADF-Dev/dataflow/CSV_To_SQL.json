{
	"name": "CSV_To_SQL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Unicorns_Raw",
						"type": "DatasetReference"
					},
					"name": "DatalakeFile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "UnicornsTable",
						"type": "DatasetReference"
					},
					"name": "SQLTable"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {Updated at} as string,",
				"          Company as string,",
				"          {Crunchbase Url} as string,",
				"          {Last Valuation (Billion $)} as string,",
				"          {Date Joined} as string,",
				"          {Year Joined} as string,",
				"          City as string,",
				"          Country as string,",
				"          Industry as string,",
				"          Investors as string,",
				"          {Company Website} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DatalakeFile",
				"DatalakeFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SQLTable"
			]
		}
	}
}